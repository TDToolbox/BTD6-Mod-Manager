<Window x:Class="BTD6_Mod_Manager.Windows.RenameWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:BTD6_Mod_Manager.Windows"
        mc:Ignorable="d"
        Title="Rename" Height="200" Width="480" ResizeMode="CanMinimize">
    <Grid>
        <Grid.RowDefinitions>

            <RowDefinition Height="29*"/>
            <RowDefinition Height="140*"/>

        </Grid.RowDefinitions>
        <TextBlock x:Name="Rename_TextBlock" HorizontalAlignment="Left" Margin="85,10,0,0" TextWrapping="Wrap" Text="What do you want to rename it to?" VerticalAlignment="Top" FontSize="18" Grid.RowSpan="2"/>
        <TextBox x:Name="Rename_TextBox" KeyDown="Rename_TextBox_KeyDown" HorizontalAlignment="Left" Height="31" Margin="20,20.75,0,0" VerticalAlignment="Top" Width="428" FontSize="18" HorizontalScrollBarVisibility="Auto" Grid.Row="1"/>
        <Button x:Name="Okay_Button" Content="Okay" HorizontalAlignment="Left" Margin="192,66.75,0,0" VerticalAlignment="Top" Width="90" Height="28" Click="Okay_Button_Click" Grid.Row="1"/>
    </Grid>
</Window>
